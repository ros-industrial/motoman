<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for sia50d:
   - 7 joints

  Usage:
    robot_interface_streaming_sia50d.launch robot_ip:=<value> controller:=<dx100>
-->
<launch>
  <arg name="robot_ip" doc="IP of controller" />
	
  <!-- controller: Controller name (dx100) -->
  <arg name="controller" value="dx100" doc="Series of the controller (dx100)" />

  <rosparam command="load" file="$(find motoman_sia50d_support)/config/joint_names_sia50d.yaml" />

  <include file="$(find motoman_driver)/launch/robot_interface_streaming_$(arg controller).launch">
    <arg name="robot_ip"   value="$(arg robot_ip)" />
  </include>
</launch>
