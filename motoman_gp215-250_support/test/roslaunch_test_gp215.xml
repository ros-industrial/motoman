<launch>
  <arg name="req_arg" value="default"/>
  <arg name="yrc1000" value="yrc1000"/>
  
  <group ns="load_gp215">
    <include file="$(find motoman_gp215-250_support)/launch/load_gp215.launch"/>
  </group>

  <group ns="test_gp215">
    <include file="$(find motoman_gp215-250_support)/launch/test_gp215.launch"/>
  </group>

  <group ns="robot_interface_streaming_gp215">
    <group ns="yrc1000" >
      <include file="$(find motoman_gp215-250_support)/launch/robot_interface_streaming_gp215.launch">
        <arg name="robot_ip"   value="$(arg req_arg)" />
        <arg name="controller" value="$(arg yrc1000)"/>
      </include>
    </group>
  </group>

  <group ns="robot_state_visualize_gp215">
    <group ns="yrc1000" >
      <include file="$(find motoman_gp215-250_support)/launch/robot_state_visualize_gp215.launch">
        <arg name="robot_ip"   value="$(arg req_arg)" />
        <arg name="controller" value="$(arg yrc1000)"/>
      </include>
    </group>
  </group>
</launch>
