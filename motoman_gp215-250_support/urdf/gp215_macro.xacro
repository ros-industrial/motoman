<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="motoman_gp215" params="prefix">
    <xacro:include filename="$(find motoman_resources)/urdf/common_materials.xacro"/>

    <!-- link list -->
    <link name="${prefix}base_link">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/base_link.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/base_link.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_1_s">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/link_1_s.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/link_1_s.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_2_l">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/link_2_l.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/link_2_l.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_3_u">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/link_3_u.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/link_3_u.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_4_r">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/link_4_r.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/link_4_r.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_5_b">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/link_5_b.dae"/>
        </geometry>
        <xacro:material_yaskawa_blue/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/link_5_b.stl"/>
        </geometry>
      </collision>
    </link>
    <link name="${prefix}link_6_t">
      <visual>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/visual/link_6_t.dae"/>
        </geometry>
        <xacro:material_yaskawa_silver/>
      </visual>
      <collision>
        <geometry>
          <mesh filename="package://motoman_gp215-250_support/meshes/gp215/collision/link_6_t.stl"/>
        </geometry>
      </collision>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint name="${prefix}joint_1_s" type="revolute">
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1_s"/>
      <origin xyz="0 0 0.650" rpy="0 0 0" />
      <axis xyz="0 0 1" />
      <limit lower="${radians(-180)}" upper="${radians(180)}" effort="9316.2" velocity="${radians(100.0)}"/>
    </joint>
    <joint name="${prefix}joint_2_l" type="revolute">
      <parent link="${prefix}link_1_s"/>
      <child link="${prefix}link_2_l"/>
      <origin xyz="0.285 0 0" rpy="0 0 0" />
      <axis xyz="0 1 0" />
      <limit lower="${radians(-60)}" upper="${radians(76)}" effort="10591.1" velocity="${radians(90.0)}"/>
    </joint>
    <joint name="${prefix}joint_3_u" type="revolute">
      <parent link="${prefix}link_2_l"/>
      <child link="${prefix}link_3_u"/>
      <origin xyz="0 0 1.150" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-77.8)}" upper="${radians(197)}" effort="9929.2" velocity="${radians(97.0)}"/>
    </joint>
    <joint name="${prefix}joint_4_r" type="revolute">
      <parent link="${prefix}link_3_u"/>
      <child link="${prefix}link_4_r"/>
      <origin xyz="1.490 0 0.250" rpy="0 0 0" />
      <axis xyz="-1 0 0" />
      <limit lower="${radians(-360)}" upper="${radians(360)}" effort="2758.0" velocity="${radians(120)}"/>
    </joint>
    <joint name="${prefix}joint_5_b" type="revolute">
      <parent link="${prefix}link_4_r"/>
      <child link="${prefix}link_5_b"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="0 -1 0" />
      <limit lower="${radians(-125)}" upper="${radians(125)}" effort="3089.0" velocity="${radians(120)}"/>
    </joint>
    <joint name="${prefix}joint_6_t" type="revolute">
      <parent link="${prefix}link_5_b"/>
      <child link="${prefix}link_6_t"/>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <axis xyz="-1 0 0" />
      <limit lower="${radians(-360)}" upper="${radians(360)}" effort="1765.1" velocity="${radians(190)}"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS-Industrial 'flange' frame: attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint name="${prefix}joint_6_t-flange" type="fixed">
      <origin xyz="0.250 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_6_t"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame: all-zeros tool frame -->
    <link name="${prefix}tool0"/>
    <joint name="${prefix}flange-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="${radians(180)} ${radians(-90)} 0"/>
      <parent link="${prefix}flange"/>
      <child link="${prefix}tool0"/>
    </joint>

    <!-- ROS base_link to Robot Manufacturer World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0.640" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>
  </xacro:macro>
</robot>
