
<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for motopos:
   - 2 joints

  Usage:
    robot_interface_streaming_motopos_mh1655.launch robot_ip:=<value> controller:=<dx200>
-->
<launch>
  <arg name="robot_ip" />

  <!-- controller: Controller name (dx200) -->
  <arg name="controller"/>

  <rosparam command="load" file="$(find motoman_motopos_mh1655_support)/config/joint_names_motopos_mh1655.yaml" />

  <include file="$(find motoman_driver)/launch/robot_interface_streaming_$(arg controller).launch">
    <arg name="robot_ip"   value="$(arg robot_ip)" />
  </include>
</launch>
