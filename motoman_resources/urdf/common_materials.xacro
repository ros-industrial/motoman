<?xml version="1.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:include filename="$(find motoman_resources)/urdf/common_colors.xacro"/>

  <xacro:macro name="material_yaskawa_blue">
    <material name="">
      <color rgba="${color_yaskawa_blue}"/>
    </material>
  </xacro:macro>

  <xacro:macro name="material_yaskawa_white">
    <material name="">
      <color rgba="${color_yaskawa_white}"/>
    </material>
  </xacro:macro>
</robot>
